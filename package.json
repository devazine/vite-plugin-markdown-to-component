{
  "name": "vite-plugin-markdown-to-component",
  "version": "1.2.2",
  "description": "Import markdown file as vue component in vite.",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsc -w -p .",
    "build": "tsc -p .",
    "lint": "eslint . --ext .ts",
    "test:acceptance": "npm run cypress:run",
    "cypress:run": "cypress run --config-file test/acceptance/cypress.json",
    "cypress:open": "cypress open --config-file test/acceptance/cypress.json"
  },
  "keywords": [
    "vite",
    "prism",
    "markdown",
    "vue"
  ],
  "author": "VaJoy Lan <vajoy@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/devazine/vite-plugin-markdown-to-component.git"
  },
  "bugs": {
    "url": "https://github.com/devazine/vite-plugin-markdown-to-component/issues"
  },
  "homepage": "https://github.com/devazine/vite-plugin-markdown-to-component/tree/main/#readme",
  "dependencies": {
    "domhandler": "^5.0.3",
    "front-matter": "^4.0.0",
    "he": "^1.2.0",
    "htmlparser2": "^6.0.0",
    "katex": "^0.15.3",
    "markdown-it": "^12.0.0",
    "markdown-it-attrs": "^4.1.3",
    "prismjs": "^1.29.0"
  },
  "peerDependencies": {
    "vite": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@types/he": "^1.1.2",
    "@types/katex": "^0.14.0",
    "@types/markdown-it": "12.0.1",
    "@types/markdown-it-attrs": "^4.1.0",
    "@types/node": "^16.0.0",
    "@types/prismjs": "^1.26.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/compiler-sfc": "3.2.26",
    "cypress": "9.2.0",
    "eslint": "7.22.0",
    "rollup": "^3.19.1",
    "typescript": "4.5.4",
    "vite": "2.4.0"
  }
}
